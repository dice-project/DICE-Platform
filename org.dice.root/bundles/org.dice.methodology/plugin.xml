<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="tocDice.xml"
            primary="true">
      </toc>
      <toc
            file="tocIntroduction.xml"
            primary="false">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.cheatsheets.cheatSheetContent">
      <category
            id="org.dice.methodology.category"
            name="DICE Methodology">
      </category>
      <category
            id="org.dice.methodology.category.tools"
            name="DICE Tools"
            parentCategory="org.dice.methodology.category">
      </category>
      <cheatsheet
            category="org.dice.methodology.category"
            composite="true"
            contentFile="cheatsheets/ArchitectureVerificationSimulationOptimization.xml"
            id="org.dice.methodology.cheatsheet"
            name="Architecture Verification, Simulation and Optimization">
         <description>
            A Cheat Sheet for DICE Architecture Verification, Simulation and Optimization
         </description>
      </cheatsheet>
      <cheatsheet
            category="org.dice.methodology.category.tools"
            composite="false"
            contentFile="cheatsheets/simulation/simulation.xml"
            id="org.dice.methodology.cheatsheet.simulation"
            name="Simulation Tool">
         <description>
            A Cheat Sheet for DICE Simulation Tool. Shows how to get started with this tool.
         </description>
      </cheatsheet>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.eclipse.ui.cheatsheets.actionSet"
            label="Cheat Sheets"
            visible="true">
         <action
               class="org.eclipse.ui.cheatsheets.CheatSheetExtensionFactory:helpMenuAction"
               id="org.eclipse.ui.cheatsheets.actions.CheatSheetHelpMenuAction"
               label="&amp;Cheat Sheets..."
               menubarPath="help/group.tutorials">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.dice.methodology.CreateModelHandler"
            commandId="org.eclipse.papyrus.uml.diagram.clazz.CreateClassDiagramCommand">
      </handler>
   </extension>
   
   

</plugin>
